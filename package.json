{
  "name": "clubhouse-desktop",
  "version": "0.0.0",
  "main": "app.js",
  "scripts": {
    "ng": "ng",
    "start": "npm-run-all -p electron:serve ng:serve",
    "build": "npm run ng:build && npm run electron:build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "ng:serve": "ng serve --host",
    "ng:build": "ng build",
    "ng:build-prod": "ng build --prod",
    "electron:serve": "wait-on tcp:4200 && electron . --serve",
    "electron:build": "electron ."
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.0",
    "@angular/cdk": "^12.2.2",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/forms": "~12.2.0",
    "@angular/localize": "~12.2.0",
    "@angular/material": "^12.2.2",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/router": "~12.2.0",
    "@ng-bootstrap/ng-bootstrap": "^10.0.0",
    "bootstrap": "^4.5.3",
    "custom-electron-titlebar": "^3.2.7",
    "electron-disable-file-drop": "^1.0.2",
    "electron-localshortcut": "^3.2.1",
    "electron-reload": "^2.0.0-alpha.1",
    "ngx-avatar": "^4.1.0",
    "ngx-bootstrap": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "wait-on": "^6.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.2",
    "@angular/cli": "~12.2.2",
    "@angular/compiler-cli": "~12.2.0",
    "@types/electron-localshortcut": "^3.1.0",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "electron": "^13.2.2",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.3.5"
  }
}
